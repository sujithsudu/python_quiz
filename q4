class Numbers:
def evenodd(list1):
even = 0
odd = 0
for i in list1:
if(int(i)%2==0):
even=even+1
else:
odd=odd+1
print(“even number count”,even)
print(“odd number count:”,odd)

list1= []
n = int(input(“Enter number of elements : “)) #array size
for i in range(0, n):
num = int(input(“enter number”)) #getting element
list1.append(int(num)) #adding to array
Numbers.evenodd(list1) #calling function
